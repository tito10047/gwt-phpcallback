<module>
  <inherits name="com.google.gwt.core.Core" />
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.json.JSON"/>

  <source path="client" />
  <super-source path="super" />


  <generate-with class="com.rpcphp.rebind.PhpRpcObjectGenerator">
    <any>
	  <when-type-assignable class="com.rpcphp.client.PhpRpcObject" />
    </any>
  </generate-with>
  <generate-with class="com.rpcphp.rebind.PhpRpcServiceGenerator">
    <any>
	  <when-type-assignable class="com.rpcphp.client.PhpRpcService" />
    </any>
  </generate-with>
</module>